<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inghackathon.dailyapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.inghackathon.dailyapp.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.inghackathon.dailyapp.ui.CartActivity"
            android:label="@string/title_activity_cart" >
        </activity>
        <activity
            android:name="com.inghackathon.dailyapp.ui.ProductActivity"
            android:label="@string/title_activity_product" >
        </activity>
        <activity
            android:name="com.inghackathon.dailyapp.ui.SelectProductActivity"
            android:label="@string/title_activity_select_product" >
        </activity>
    </application>

</manifest>
